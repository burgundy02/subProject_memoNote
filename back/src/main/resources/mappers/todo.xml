        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.memorization_note.repository.TodoMapper">

    <insert id="save">
        insert into memo_tb
        values
            (0, #{question}, #{answer}, #{explain}, now(). now())
    </insert>
</mapper>